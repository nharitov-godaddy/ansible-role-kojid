#<domain> <type> <item> <value>
{% if kojid_ulimits_conf %}
kojibuilder soft memlock {{ kojid_ulimits_memlock }}
kojibuilder hard memlock {{ kojid_ulimits_memlock }}
kojibuilder soft nofile  {{ kojid_ulimits_nofiles }}
kojibuilder hard nofile  {{ kojid_ulimits_nofiles }}
{% else %}
*         -      nofile 10240
{% endif %}
